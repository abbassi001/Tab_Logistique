{% extends 'base.html.twig' %}

{% block title %}DocumentSupport{% endblock %}

{% block body %}
    <h1>DocumentSupport</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ document_support.id }}</td>
            </tr>
            <tr>
                <th>Nom_fichier</th>
                <td>{{ document_support.nomFichier }}</td>
            </tr>
            <tr>
                <th>Type_document</th>
                <td>{{ document_support.typeDocument }}</td>
            </tr>
            <tr>
                <th>Date_upload</th>
                <td>{{ document_support.dateUpload ? document_support.dateUpload|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Chemin_stockage</th>
                <td>{{ document_support.cheminStockage }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_document_support_index') }}">back to list</a>

    <a href="{{ path('app_document_support_edit', {'id': document_support.id}) }}">edit</a>

    {{ include('document_support/_delete_form.html.twig') }}
{% endblock %}
